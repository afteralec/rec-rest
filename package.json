{
  "name": "rest",
  "version": "0.0.1",
  "description": "A Restaurant API for Rec",
  "main": "dist/index.js",
  "dependencies": {
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "knex": "^3.1.0",
    "luxon": "^3.4.4",
    "npm": "^10.4.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.17",
    "axios": "^1.6.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "scripts": {
    "dev": "ts-node ./src/index.ts",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "env:setup": "cp ./.env.example ./.env",
    "db:setup": "rm ./db/rest.db && cat ./db/schema.sql | sqlite3 ./db/rest.db",
    "db:seed": "cat ./db/seed.sql | sqlite3 ./db/rest.db"
  },
  "author": "Alec DuBois",
